{
  "name": "thuocsi.vn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 80",
    "start-local": "next start -p 8000",
    "start-debug": "NODE_OPTIONS='--inspect' next start -p 8000",
    "build:local": "next build && NODE_OPTIONS='--inspect' next start -p 8000",
    "prd": "yarn build  && yarn start",
    "test": "cross-env NODE_ENV=test mocha",
    "lint:check": "eslint --ext=js,jsx src",
    "lint:fix": "eslint --ext=js,jsx --fix src",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf yarn.lock && yarn install && yarn dev",
    "git-reset": "git reset --hard HEAD",
    "checkEslintAndBuild": "yarn lint:check && yarn build",
    "cm": "git add . && git commit -m",
    "clear-branch": "git branch | grep -v 'dev' | xargs git branch -D"
  },
  "pre-commit": "checkEslintAndBuild",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "clsx": "latest",
    "js-cookie": "^2.2.1",
    "next": "10.0.7",
    "next-i18next": "^8.1.2",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-slick": "^0.27.13",
    "react-toastify": "^6.2.0",
    "react-transition-group": "^4.4.1",
    "react-upload-gallery": "^0.2.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "pre-commit": "^1.2.2"
  }
}